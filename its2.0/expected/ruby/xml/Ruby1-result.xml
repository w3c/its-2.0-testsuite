<?xml version="1.0" encoding="utf-8"?>
<nodeList xmlns:its="http://www.w3.org/2005/11/its"
          xmlns:datc="http://example.com/datacats">
   <nodeList datacat="ruby">
      <node path="/text" outputType="no-value">
         <output/>
      </node>
      <node path="/text/@its:version" outputType="no-value">
         <output/>
      </node>
      <node path="/text/body[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/text/body[1]/p[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/text/body[1]/p[1]/its:ruby[1]" outputType="new-value-local">
         <output>
            <its:ruby>
               <its:rb>慶応義塾大学</its:rb>
               <its:rp>(</its:rp>
               <its:rt>けいおうぎじゅくだいがく</its:rt>
               <its:rp>)</its:rp>
            </its:ruby>
         </output>
      </node>
      <node path="/text/body[1]/p[1]/its:ruby[1]/its:rb[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/text/body[1]/p[1]/its:ruby[1]/its:rp[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/text/body[1]/p[1]/its:ruby[1]/its:rt[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/text/body[1]/p[1]/its:ruby[1]/its:rp[2]" outputType="no-value">
         <output/>
      </node>
   </nodeList>
</nodeList>
